{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.AspNetCore": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "MongoDb": "mongodb://localhost:27017"
    }
}
